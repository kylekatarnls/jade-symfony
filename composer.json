    "require": {
        "pug-php/pug": "^2.0",
        "pug-php/pug-assets": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Kyle Katarn",
            "email": "kylekatarnls@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": {
            "Jade": "src/",
            "Pug": "src/"
        }
    }
}
